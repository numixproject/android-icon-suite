<resources>
		<!-- Label for app name in launcher -->
		<!-- Edit these two strings to match your theme name -->
	<string name="app_name">Numix Circle</string>
	<string name="theme_name">Numix Icon Theme - CIRCLE</string>
	
		<!-- Apex Previews -->
	<string name="theme_feature">theme_header</string>
  	<string name="theme_preview1">theme_preview1</string>
  	<string name="theme_preview2">theme_preview2</string>
 	<string name="theme_preview3">theme_preview3</string>
  	<string name="theme_preview4">theme_preview4</string>
  	<string name="theme_preview5">theme_preview5</string>
  	
		<!-- Apex Theme settings -->
	<bool name="config_apexskin">true</bool>
	<bool name="config_iconpack">true</bool>
		<!-- This string shows up inside the setting to Apply the theme on Apex Launcher. I don't recommend changing it -->
	<string name="theme_description">"Open this theme from the app drawer to change wallpaper, find us on social sites, rate theme, or just to check our other apps!"</string>
  	
		<!-- ADW Stuff -->
		<!-- 1=top dots, 2=top slider, 3=bottom slider, 4=ics slider -->
	<string name="config_desktop_indicator_type">4</string>
		<!-- Force the theme to use an specific dock style -->
		<!--  0=None, 1=3buttons, 2=5buttons, 3=1button -->
	<string name="main_dock_style">2</string>
	
  		<!-- Go Launcher stuff -->
	<string name="theme_title">Numix Icon Theme - Circle</string>
	<string name="theme_info">All the icons are circular and pack utilises the use of inset and dropshadows and subtle gradients.</string>
	<bool name="enableIconPack">true</bool>
	<bool name="enableDockPack">false</bool>
	<string name="go_applied">Thanks for using Numix Icon Theme - Circle. Your theme is being set!</string>
		
</resources>